#!/bin/bash --login

#SBATCH -J dalton

#SBATCH -t 00:10:00

# number of nodes
#SBATCH -N 2

# number of MPI processes
#SBATCH -n 64

#SBATCH -o stdout.txt
#SBATCH -e stderr.txt

module load dalton/2013.4

export DALTON_TMPDIR=$SNIC_TMP/dalton_scratch/$SLURM_JOB_ID
export OMP_NUM_THREADS=1
export LSDALTON_LAUNCHER="aprun -n 64"

cd $SLURM_SUBMIT_DIR
lsdalton b3lyp_energy.dal h2o2.mol
